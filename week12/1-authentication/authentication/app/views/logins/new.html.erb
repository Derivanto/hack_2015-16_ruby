<h3> Login </h3>

<%= form_tag '/login' do %>
    
    <%= label_tag :email %> <br> <%= text_field_tag :email %> <br>
    <%= label_tag :password %> <br> <%= password_field_tag :password %> <br>
    <%= label_tag :remember_me %>  <%= check_box_tag :remember_me %> <br>
    <%= submit_tag "Login" %> 

<% end %>
